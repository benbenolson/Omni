<deviceStitchings
   xmlns="http://www.ibm.com/linux/ltc/projects/omni/"
   xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
   xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <deviceStitching>
      <StitchingPosition>1</StitchingPosition>
      <StitchingReferenceEdge>Top</StitchingReferenceEdge>
      <StitchingType>Corner</StitchingType>
      <StitchingCount>1</StitchingCount>
      <StitchingAngle>0</StitchingAngle>
      <command>_NUL_</command>
      <deviceID>a</deviceID>
   </deviceStitching>
   <deviceStitching>
      <StitchingPosition>5</StitchingPosition>
      <StitchingReferenceEdge>Right</StitchingReferenceEdge>
      <StitchingType>Saddle</StitchingType>
      <StitchingCount>3</StitchingCount>
      <StitchingAngle>45</StitchingAngle>
      <command>_NUL_</command>
      <deviceID>b</deviceID>
   </deviceStitching>
</deviceStitchings>
