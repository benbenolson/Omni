<deviceScalings
   xmlns="http://www.ibm.com/linux/ltc/projects/omni/"
   xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
   xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <deviceScaling>
      <default>50</default>
      <minimum>1</minimum>
      <maximum>100</maximum>
      <command>_NUL_</command>
      <allowedType>FitToPage</allowedType>
      <deviceID>a</deviceID>
   </deviceScaling>
   <deviceScaling>
      <default>73</default>
      <minimum>10</minimum>
      <maximum>90</maximum>
      <command>_NUL_</command>
      <allowedType>RotateAndOrFit</allowedType>
      <deviceID>b</deviceID>
   </deviceScaling>
   <deviceScaling>
      <default>100</default>
      <minimum>100</minimum>
      <maximum>100</maximum>
      <command>_NUL_</command>
      <allowedType>Clip</allowedType>
      <deviceID>c</deviceID>
   </deviceScaling>
</deviceScalings>
