<Device xmlns="http://www.ibm.com/linux/ltc/projects/omni/" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" name="HP LaserJet 6L/6L Gold" xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
<DriverName>HP LaserJet</DriverName>
<Capability type="MONOCHROME"/>
<Capability type="MIRROR"/>
<RasterCapabilities type="TOP_TO_BOTTOM"/>
<DeviceOptions type="DO_AUTO_ROTATE"/>
<DeviceOptions type="DO_SCALABLE_FONT"/>
<DeviceOptions type="DO_SUPPORT_DUPLEX"/>
<DeviceOptions type="DO_SUPPORT_RULES"/>
<DeviceOptions type="DO_SUPPORT_DOT_COORDS"/>
<DeviceOptions type="DO_SUPPORT_PJL"/>
<DeviceOptions type="DO_SUPPORT_JOBOFFSET"/>
<DeviceOptions type="DO_SUPPORT_PATTERNS"/>
<DeviceOptions type="DO_SUPPORT_PCL5"/>
<DeviceOptions type="DO_MIXED_TRAY"/>
<DeviceOptions type="DO_SUPPORT_PAGEPROT_ESC"/>
<DeviceOptions type="DO_SUPPORT_HPGL2"/>
<DeviceOptions type="DO_SUPPORT_HPGL2_MC"/>
<DeviceOptions type="DO_PAGEPROT_AUTO"/>
<DeviceOptions type="DO_SUPPORT_COMPRESS5"/>
<DeviceOptions type="DO_PRINTER_DISPLAY"/>
<DeviceOptions type="DO_SUPPORT_WATERMARK"/>
<DeviceOptions type="DO_SUPPORT_BIDI"/>
<DeviceOptions type="DO_SUPPORT_PCLXL"/>
<DeviceOptions type="DO_SUPPORT_AUTOSELECT"/>
<DeviceOptions type="DO_SUPPORT_PCLXL_DEFAULT"/>
<DeviceOptions type="DO_SUPPORT_PCLXL_1200"/>
<PDL level="PDL_PCL" sublevel="LEVEL_PCL3" major="1" minor="0"/>
<Uses>HP LaserJet PCL Commands.xml</Uses>
<Has>HP LaserJet Orientations.xml</Has>
<Has>HP LaserJet 6L_6L Gold Resolutions.xml</Has>
<Has>HP LaserJet PCL Monochrome PrintModes.xml</Has>
<Has>HP LaserJet 4L Trays.xml</Has>
<Has>HP LaserJet III Forms.xml</Has>
<Has>HP LaserJet Classic Medias.xml</Has>
<Has>HP LaserJet 5L Connections.xml</Has>
   <Has>HP LaserJet Copies.xml</Has>
   <Has>HP LaserJet NUps.xml</Has>
   <Has>HP LaserJet OutputBins.xml</Has>
   <Has>HP LaserJet Scalings.xml</Has>
   <Has>HP LaserJet SheetCollates.xml</Has>
   <Has>HP LaserJet Sides.xml</Has>
   <Has>HP LaserJet Stitchings.xml</Has>
   <Has>HP LaserJet Trimmings.xml</Has>
<Instance>HP LaserJet PCL Instance.hpp</Instance>
<Instance>HP LaserJet PCL Instance.cpp</Instance>
<Blitter>HP LaserJet PCL Blitter.hpp</Blitter>
<Blitter>HP LaserJet PCL Blitter.cpp</Blitter>
<DefaultJobProperties>
      <Copies>1</Copies>
      <dither>DITHER_STUCKI_DIFFUSION</dither>
      <Form>na_letter_8.50x11.00in</Form>
      <omniForm>FORM_LETTER</omniForm>
      <media>MEDIA_PLAIN</media>
      <NumberUp>
         <NumberUp FORMAT="XbyY">
            <x>1</x>
            <y>1</y>
         </NumberUp>
         <NumberUpDirection>TorightTobottom</NumberUpDirection>
      </NumberUp>
      <Rotation>Portrait</Rotation>
      <omniOrientation>ORIENTATION_PORTRAIT</omniOrientation>
      <OutputBin>Top</OutputBin>
      <printmode>PRINT_MODE_1_ANY</printmode>
      <Resolution>300x300</Resolution>
      <omniResolution>RESOLUTION_300_X_300</omniResolution>
      <Scaling>
         <ScalingPercentage>100</ScalingPercentage>
         <ScalingType>None</ScalingType>
      </Scaling>
      <SheetCollate>SheetUncollated</SheetCollate>
      <Sides>OneSidedFront</Sides>
      <Stitching>
         <StitchingPosition>0</StitchingPosition>
         <StitchingReferenceEdge>Top</StitchingReferenceEdge>
         <StitchingType>None</StitchingType>
         <StitchingCount>0</StitchingCount>
         <StitchingAngle>0</StitchingAngle>
      </Stitching>
      <InputTray>Tray</InputTray>
      <omniTray>TRAY_TRAY</omniTray>
      <Trimming>None</Trimming>
   </DefaultJobProperties>
</Device>
