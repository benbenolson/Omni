<Device xmlns="http://www.ibm.com/linux/ltc/projects/omni/" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" name="Kyocera F-1000A/F-1000" xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
<DriverName>Kyocera</DriverName>
<Capability type="MONOCHROME"/>
<Capability type="MIRROR"/>
<RasterCapabilities type="TOP_TO_BOTTOM"/>
<DeviceOptions type="DO_SUPPORT_DOT_COORDS"/>
<PDL level="PDL_PCL" sublevel="LEVEL_PCL3" major="1" minor="0"/>
<Uses>Kyocera PCL Commands.xml</Uses>
<Has>Kyocera Orientations.xml</Has>
<Has>Kyocera F-1000A_F-1000 Resolutions.xml</Has>
<Has>Kyocera PCL PrintModes.xml</Has>
<Has>Kyocera F-1000A_F-1000 Trays.xml</Has>
<Has>Kyocera F-1000A_F-1000 Forms.xml</Has>
<Has>Kyocera F-1000A_F-1000 Medias.xml</Has>
<Has>Kyocera F-1000A_F-1000 Connections.xml</Has>
   <Has>Kyocera Copies.xml</Has>
   <Has>Kyocera NUps.xml</Has>
   <Has>Kyocera OutputBins.xml</Has>
   <Has>Kyocera Scalings.xml</Has>
   <Has>Kyocera SheetCollates.xml</Has>
   <Has>Kyocera Sides.xml</Has>
   <Has>Kyocera Stitchings.xml</Has>
   <Has>Kyocera Trimmings.xml</Has>
<Instance>Kyocera PCL Instance.hpp</Instance>
<Instance>Kyocera PCL Instance.cpp</Instance>
<Blitter>Kyocera PCL Blitter.hpp</Blitter>
<Blitter>Kyocera PCL Blitter.cpp</Blitter>
<DefaultJobProperties>
      <Copies>1</Copies>
      <dither>DITHER_STUCKI_DIFFUSION</dither>
      <Form>na_letter_8.50x11.00in</Form>
      <omniForm>FORM_LETTER</omniForm>
      <media>MEDIA_PLAIN</media>
      <NumberUp>
         <NumberUp FORMAT="XbyY">
            <x>1</x>
            <y>1</y>
         </NumberUp>
         <NumberUpDirection>TorightTobottom</NumberUpDirection>
      </NumberUp>
      <Rotation>Portrait</Rotation>
      <omniOrientation>ORIENTATION_PORTRAIT</omniOrientation>
      <OutputBin>Top</OutputBin>
      <printmode>PRINT_MODE_1_ANY</printmode>
      <Resolution>300x300</Resolution>
      <omniResolution>RESOLUTION_300_X_300</omniResolution>
      <Scaling>
         <ScalingPercentage>100</ScalingPercentage>
         <ScalingType>None</ScalingType>
      </Scaling>
      <SheetCollate>SheetUncollated</SheetCollate>
      <Sides>OneSidedFront</Sides>
      <Stitching>
         <StitchingPosition>0</StitchingPosition>
         <StitchingReferenceEdge>Top</StitchingReferenceEdge>
         <StitchingType>None</StitchingType>
         <StitchingCount>0</StitchingCount>
         <StitchingAngle>0</StitchingAngle>
      </Stitching>
      <InputTray>Tray</InputTray>
      <omniTray>TRAY_TRAY</omniTray>
      <Trimming>None</Trimming>
   </DefaultJobProperties>
</Device>
