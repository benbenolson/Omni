<deviceCommands
   xmlns="http://www.ibm.com/linux/ltc/projects/omni/"
   xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
   xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <command name="cmdSetUnitsOfMeasure">_ESC_ "&amp;u%nD"</command>
   <command name="cmdConfigureRasterDataHeader">_ESC_ "*g%nW"</command>
   <command name="cmdConfigureRasterDataParam1">"%c%c"</command>
   <command name="cmdConfigureRasterDataParam2">"%W%W%W"</command>
   <command name="cmdEnterLanguage">_ESC_ "%-12345X@PJL ENTER LANGUAGE=PCL " _LF_</command>
   <command name="cmdInit">_ESC_ "E" _ESC_  "&amp;k0G"</command>
   <command name="cmdTerm">_ESC_ "E"</command>
   <command name="cmdReset">_ESC_ "E"</command>
   <command name="cmdAbort">_ESC_ "E" _ESC_  "&amp;k0G"</command>
   <command name="cmdPageEject">_FF_</command>
   <command name="cmdPortrait">_ESC_  "&amp;l0O"</command>
   <command name="cmdLandscape">_ESC_  "&amp;l1O"</command>
   <command name="cmdSetXYPos">_ESC_ "*p%nx%nY"</command>
   <command name="cmdSetXPos">_ESC_ "*p%nX"</command>
   <command name="cmdSetYPos">_ESC_ "*p%nY"</command>
   <command name="cmdSelfTest">_ESC_ "Z"</command>
   <command name="cmdEOLWrapOn">_ESC_  "&amp;s0C"</command>
   <command name="cmdEOLWrapOff">_ESC_  "&amp;s1C"</command>
   <command name="cmdLineTerm">_ESC_ "%k%nG"</command>
   <command name="cmdTextLtoR">_ESC_  "&amp;k0W"</command>
   <command name="cmdBidi">_ESC_  "&amp;K1W"</command>
   <command name="cmdTextScaleOn">_ESC_  "&amp;k5W"</command>
   <command name="cmdTextScaleOff">_ESC_  "&amp;k6W"</command>
   <command name="cmdSetTopMargin">_ESC_  "&amp;l0E"</command>
   <command name="cmdSetResolution">_ESC_ "*t%nR"</command>
   <command name="cmdBeginRasterGraphics">_ESC_ "*r%nA"</command>
   <command name="cmdEndRasterGraphics">_ESC_ "*rC"</command>
   <command name="cmdSetCompression">_ESC_ "*b%nM"</command>
   <command name="cmdTransferRasterBlock">_ESC_ "*b%nW"</command>
   <command name="cmdTransferRasterPlane">_ESC_ "*b%nV"</command>
   <command name="cmdConfigureImageData1BPP">_ESC_ "*v6W" HEX (00) HEX (01) HEX (01) HEX (08) HEX (08) HEX (08)</command>
   <command name="cmdConfigureImageData24BPP">_ESC_ "*v6W" HEX (00) HEX (03) HEX (00) HEX (08) HEX (08) HEX (08)</command>
   <command name="cmdSetSourceRasterWidth">_ESC_ "*r%nS"</command>
   <command name="cmdSetSourceRasterHeight">_ESC_ "*r%nT"</command>
   <command name="cmdSetDestinationRasterWidth">_ESC_ "*t%fH"</command>
   <command name="cmdSetDestinationRasterHeight">_ESC_ "*t%fV"</command>
   <command name="cmdSetColorLookupTable">_ESC_ "*l%nW"</command>
   <command name="cmdSetColorLookupTableDataParm">"%c"</command>
</deviceCommands>
