<Device xmlns="http://www.ibm.com/linux/ltc/projects/omni/" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" name="IBM Infoprint 1356J" xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <DriverName>IBM</DriverName>
   <Capability type="MONOCHROME"/>
   <Capability type="MIRROR"/>
   <RasterCapabilities type="TOP_TO_BOTTOM"/>
   <DeviceOptions type="SUPPORTS_PJL"/>
   <DeviceOptions type="SUPPORTS_EMULATIONMODE"/>
   <PDL level="PDL_RPDL" sublevel="PDL_DONTCARE" major="1" minor="0"/>
   <Uses>IBM Infoprint 1356J Commands.xml</Uses>
   <Uses>IBM Orientations.xml</Uses>
   <Has>IBM Infoprint 1356J Resolutions.xml</Has>
   <Has>IBM Infoprint 1356J PrintModes.xml</Has>
   <Has>IBM Infoprint 1356J Trays.xml</Has>
   <Has>IBM Infoprint 1356J Forms.xml</Has>
   <Has>IBM Infoprint 1356J Medias.xml</Has>
   <Has>IBM Infoprint 1356J Connections.xml</Has>
   <Has>IBM Copies.xml</Has>
   <Has>IBM NUps.xml</Has>
   <Has>IBM Infoprint 1356J OutputBins.xml</Has>
   <Has>IBM Scalings.xml</Has>
   <Has>IBM SheetCollates.xml</Has>
   <Has>IBM Sides.xml</Has>
   <Has>IBM Stitchings.xml</Has>
   <Has>IBM Trimmings.xml</Has>
   <Instance>IBM RPDL Instance.hpp</Instance>
   <Instance>IBM RPDL Instance.cpp</Instance>
   <Blitter>IBM RPDL Blitter.hpp</Blitter>
   <Blitter>IBM RPDL Blitter.cpp</Blitter>
   <DefaultJobProperties>
      <Copies>1</Copies>
      <dither>DITHER_STUCKI_DIFFUSION</dither>
      <Form>iso_a4_210.00x297.00mm</Form>
      <omniForm>FORM_A4</omniForm>
      <media>MEDIA_PLAIN</media>
      <NumberUp>
         <NumberUp FORMAT="XbyY">
            <x>1</x>
            <y>1</y>
         </NumberUp>
         <NumberUpPresentationDirection>TorightTobottom</NumberUpPresentationDirection>
      </NumberUp>
      <Rotation>Portrait</Rotation>
      <omniOrientation>ORIENTATION_PORTRAIT</omniOrientation>
      <OutputBin>Top</OutputBin>
      <printmode>PRINT_MODE_1_ANY</printmode>
      <Resolution>600x600</Resolution>
      <omniResolution>RESOLUTION_600_X_600</omniResolution>
      <Scaling>
         <ScalingPercentage>0</ScalingPercentage>
         <ScalingType>None</ScalingType>
      </Scaling>
      <SheetCollate>SheetUncollated</SheetCollate>
      <Sides>OneSidedFront</Sides>
      <Stitching>
         <StitchingPosition>0</StitchingPosition>
         <StitchingReferenceEdge>Top</StitchingReferenceEdge>
         <StitchingType>None</StitchingType>
         <StitchingCount>0</StitchingCount>
         <StitchingAngle>0</StitchingAngle>
      </Stitching>
      <InputTray>AutoSelect</InputTray>
      <omniTray>TRAY_AUTO</omniTray>
      <Trimming>None</Trimming>
   </DefaultJobProperties>
</Device>
