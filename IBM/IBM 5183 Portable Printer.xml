<Device xmlns="http://www.ibm.com/linux/ltc/projects/omni/" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" name="IBM 5183 Portable Printer" xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <DriverName>IBM</DriverName>
   <Capability type="MONOCHROME"/>
   <Capability type="MIRROR"/>
   <RasterCapabilities type="TOP_TO_BOTTOM"/>
   <PDL level="PDL_Epson" sublevel="LEVEL_ESC" major="1" minor="0"/>
   <Uses>IBM 5183 Portable Printer Commands.xml</Uses>
   <Has>IBM Orientations.xml</Has>
   <Has>IBM 5183 Portable Printer Resolutions.xml</Has>
   <Has>IBM 5183 Portable Printer PrintModes.xml</Has>
   <Has>IBM 5183 Portable Printer Trays.xml</Has>
   <Has>IBM 5183 Portable Printer Forms.xml</Has>
   <Has>IBM 5183 Portable Printer Medias.xml</Has>
   <Has>IBM 5183 Portable Printer Connections.xml</Has>
   <Has>IBM Copies.xml</Has>
   <Has>IBM NUps.xml</Has>
   <Has>IBM OutputBins.xml</Has>
   <Has>IBM Scalings.xml</Has>
   <Has>IBM SheetCollates.xml</Has>
   <Has>IBM Sides.xml</Has>
   <Has>IBM Stitchings.xml</Has>
   <Has>IBM Trimmings.xml</Has>
   <Instance>IBM ESC Instance.hpp</Instance>
   <Instance>IBM ESC Instance.cpp</Instance>
   <Blitter>IBM ESC Blitter.hpp</Blitter>
   <Blitter>IBM ESC Blitter.cpp</Blitter>
   <DefaultJobProperties>
      <Copies>1</Copies>
      <dither>DITHER_STUCKI_DIFFUSION</dither>
      <Form>na_letter_8.50x11.00in</Form>
      <omniForm>FORM_LETTER</omniForm>
      <media>MEDIA_PLAIN</media>
      <NumberUp>
         <NumberUp FORMAT="XbyY">
            <x>1</x>
            <y>1</y>
         </NumberUp>
         <NumberUpDirection>TorightTobottom</NumberUpDirection>
      </NumberUp>
      <Rotation>Portrait</Rotation>
      <omniOrientation>ORIENTATION_PORTRAIT</omniOrientation>
      <OutputBin>Top</OutputBin>
      <printmode>PRINT_MODE_1_ANY</printmode>
      <Resolution>180x180</Resolution>
      <omniResolution>RESOLUTION_180_X_180</omniResolution>
      <Scaling>
         <ScalingPercentage>100</ScalingPercentage>
         <ScalingType>None</ScalingType>
      </Scaling>
      <SheetCollate>SheetUncollated</SheetCollate>
      <Sides>OneSidedFront</Sides>
      <Stitching>
         <StitchingPosition>0</StitchingPosition>
         <StitchingReferenceEdge>Top</StitchingReferenceEdge>
         <StitchingType>None</StitchingType>
         <StitchingCount>0</StitchingCount>
         <StitchingAngle>0</StitchingAngle>
      </Stitching>
      <InputTray>Continuous</InputTray>
      <omniTray>TRAY_CSF</omniTray>
      <Trimming>None</Trimming>
   </DefaultJobProperties>
</Device>
