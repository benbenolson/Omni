<Device xmlns="http://www.ibm.com/linux/ltc/projects/omni/" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" name="Canon BJC-7100" xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <DriverName>Canon</DriverName>
   <Capability type="COLOR"/>
   <Capability type="MONOCHROME"/>
   <Capability type="MIRROR"/>
   <RasterCapabilities type="TOP_TO_BOTTOM"/>
   <PDL level="PDL_CaPSL" sublevel="PDL_DONTCARE" major="1" minor="0"/>
   <Uses>Canon BJ-30 Commands.xml</Uses>
   <Has>Canon Orientations.xml</Has>
   <Has>Canon BJC-7000 Resolutions.xml</Has>
   <Has>Canon BJC-7100 PrintModes.xml</Has>
   <Has>Canon BJ-30 Trays.xml</Has>
   <Has>Canon BJC-7100 Forms.xml</Has>
   <Has>Canon BJ-30 Medias.xml</Has>
   <Has>Canon BJC-7100 Connections.xml</Has>
   <Has>Canon BJC-7100 Gammas.xml</Has>
   <Has>Canon BJC-7100 Data.xml</Has>
   <Has>Canon Copies.xml</Has>
   <Has>Canon NUps.xml</Has>
   <Has>Canon OutputBins.xml</Has>
   <Has>Canon Scalings.xml</Has>
   <Has>Canon SheetCollates.xml</Has>
   <Has>Canon Sides.xml</Has>
   <Has>Canon Stitchings.xml</Has>
   <Has>Canon Trimmings.xml</Has>
   <Instance>Canon Instance.hpp</Instance>
   <Instance>Canon Instance.cpp</Instance>
   <Blitter>Canon Blitter.hpp</Blitter>
   <Blitter>Canon Blitter.cpp</Blitter>
   <DefaultJobProperties>
      <Copies>1</Copies>
      <dither>DITHER_STUCKI_DIFFUSION</dither>
      <Form>na_letter_8.50x11.00in</Form>
      <omniForm>FORM_LETTER</omniForm>
      <media>MEDIA_PLAIN</media>
      <NumberUp>
         <NumberUp FORMAT="XbyY">
            <x>1</x>
            <y>1</y>
         </NumberUp>
         <NumberUpDirection>TorightTobottom</NumberUpDirection>
      </NumberUp>
      <Rotation>Portrait</Rotation>
      <omniOrientation>ORIENTATION_PORTRAIT</omniOrientation>
      <OutputBin>Top</OutputBin>
      <printmode>PRINT_MODE_24_CMYK</printmode>
      <Resolution>300x300</Resolution>
      <omniResolution>RESOLUTION_DRAFT</omniResolution>
      <Scaling>
         <ScalingPercentage>100</ScalingPercentage>
         <ScalingType>None</ScalingType>
      </Scaling>
      <SheetCollate>SheetUncollated</SheetCollate>
      <Sides>OneSidedFront</Sides>
      <Stitching>
         <StitchingPosition>0</StitchingPosition>
         <StitchingReferenceEdge>Top</StitchingReferenceEdge>
         <StitchingType>None</StitchingType>
         <StitchingCount>0</StitchingCount>
         <StitchingAngle>0</StitchingAngle>
      </Stitching>
      <InputTray>Continuous</InputTray>
      <omniTray>TRAY_AUTO_FEEDER</omniTray>
      <Trimming>None</Trimming>
   </DefaultJobProperties>
</Device>
