<deviceResolutions xmlns="http://www.ibm.com/linux/ltc/projects/omni/" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <deviceResolution>
      <name>300x300</name>
      <omniName>RESOLUTION_DRAFT</omniName>
      <xRes>300</xRes>
      <yRes>300</yRes>
      <command>_ESC_ &quot;(d&quot; HEX (04) _NUL_ HEX (01) &quot;,&quot; HEX (01) &quot;,&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>1</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>600x600</name>
      <omniName>RESOLUTION_NORMAL</omniName>
      <xRes>600</xRes>
      <yRes>600</yRes>
      <command>_ESC_ &quot;(d&quot; HEX (04) _NUL_ HEX (02) &quot;X&quot; HEX (02) &quot;X&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>1</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>1200x600</name>
      <omniName>RESOLUTION_PRESENTATION</omniName>
      <xRes>1200</xRes>
      <yRes>600</yRes>
      <command>_ESC_ &quot;(d&quot; HEX (04) _NUL_ HEX (02) &quot;X&quot; HEX (04) HEX (B0)</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>1</resolutionScanlineMultiple>
   </deviceResolution>
</deviceResolutions>
