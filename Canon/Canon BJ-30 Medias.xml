<deviceMedias
   xmlns="http://www.ibm.com/linux/ltc/projects/omni/"
   xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
   xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <deviceMedia>
      <name>MEDIA_PLAIN</name>
      <command>_NUL_</command>
      <mediaColorAdjustRequired>0</mediaColorAdjustRequired>
      <mediaAbsorption>MEDIA_HEAVY_ABSORPTION</mediaAbsorption>
   </deviceMedia>
   <deviceMedia>
      <name>MEDIA_COATED</name>
      <command>HEX (01)</command>
      <mediaColorAdjustRequired>0</mediaColorAdjustRequired>
      <mediaAbsorption>MEDIA_LIGHT_ABSORPTION</mediaAbsorption>
   </deviceMedia>
   <deviceMedia>
      <name>MEDIA_TRANSPARENCY</name>
      <command>HEX (02)</command>
      <mediaColorAdjustRequired>0</mediaColorAdjustRequired>
      <mediaAbsorption>MEDIA_NO_ABSORPTION</mediaAbsorption>
   </deviceMedia>
   <deviceMedia>
      <name>MEDIA_BACKPRINT</name>
      <command>HEX (03)</command>
      <mediaColorAdjustRequired>0</mediaColorAdjustRequired>
      <mediaAbsorption>MEDIA_LIGHT_ABSORPTION</mediaAbsorption>
   </deviceMedia>
   <deviceMedia>
      <name>MEDIA_GLOSSY</name>
      <command>HEX (05)</command>
      <mediaColorAdjustRequired>0</mediaColorAdjustRequired>
      <mediaAbsorption>MEDIA_NO_ABSORPTION</mediaAbsorption>
   </deviceMedia>
   <deviceMedia>
      <name>MEDIA_HIGH_GLOSS_FILM</name>
      <command>HEX (06)</command>
      <mediaColorAdjustRequired>0</mediaColorAdjustRequired>
      <mediaAbsorption>MEDIA_NO_ABSORPTION</mediaAbsorption>
   </deviceMedia>
   <deviceMedia>
      <name>MEDIA_CLOTH</name>
      <command>HEX (04)</command>
      <mediaColorAdjustRequired>0</mediaColorAdjustRequired>
      <mediaAbsorption>MEDIA_HEAVY_ABSORPTION</mediaAbsorption>
   </deviceMedia>
</deviceMedias>
