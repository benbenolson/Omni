<deviceResolutions xmlns="http://www.ibm.com/linux/ltc/projects/omni/" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <deviceResolution>
      <name>180x180</name>
      <omniName>RESOLUTION_180_X_180</omniName>
      <xRes>180</xRes>
      <yRes>180</yRes>
      <command>_ESC_ &quot;(d&quot; HEX2S(00,04) HEX(00) HEX(B4) HEX(00) HEX(B4)</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>1</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>360x360</name>
      <omniName>RESOLUTION_360_X_360</omniName>
      <xRes>360</xRes>
      <yRes>360</yRes>
      <command>_ESC_ &quot;(d&quot; HEX2S(00,04) HEX(01) HEX(68) HEX(01) HEX(68)</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>1</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>720x360</name>
      <omniName>RESOLUTION_720_X_360</omniName>
      <xRes>720</xRes>
      <yRes>360</yRes>
      <command>_ESC_ &quot;(d&quot; HEX (04) _NUL_ HEX (01) &quot;h&quot; HEX (02) HEX (D0)</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>1</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>720x720</name>
      <omniName>RESOLUTION_720_X_720</omniName>
      <xRes>720</xRes>
      <yRes>720</yRes>
      <command>_ESC_ &quot;(d&quot; HEX2S(00,04) HEX(02) HEX(D0) HEX(02) HEX(D0)</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>1</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>1440x720</name>
      <omniName>RESOLUTION_1440_X_720</omniName>
      <xRes>1440</xRes>
      <yRes>720</yRes>
      <command>_ESC_ &quot;(d&quot; HEX2S(00,04) HEX(02) HEX(D0) HEX(05) HEX(A0)</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>318</resolutionScanlineMultiple>
   </deviceResolution>
</deviceResolutions>
