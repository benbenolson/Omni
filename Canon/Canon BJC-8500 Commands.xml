<deviceCommands
   xmlns="http://www.ibm.com/linux/ltc/projects/omni/"
   xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
   xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <command name="cmdSetMono">_ESC_ "(c" HEX2S(00,01) HEX(01)</command>
   <command name="cmdSetPageID">_ESC_ "(q" HEX2S(00,01) HEX(01)</command>
   <command name="cmdBJLInit">_NUL_ _NUL_ _NUL_ _ESC_ "[K" HEX2S(00,02) HEX(00) HEX(1f)</command>
   <command name="cmdBJLCommon">"ControlMode=Common" _LF_</command>
   <command name="cmdBJLModeBJ">"ControlMode=BJ" _LF_</command>
   <command name="cmdBJLModeLQ">"ControlMode=LQ" _LF_</command>
   <command name="cmdBJLStart">_NUL_ _NUL_ _NUL_ "BJLSTART" _LF_</command>
   <command name="cmdBJLEnd">"BJLEND" _LF_</command>
   <command name="cmdInit">_NUL_ _NUL_ _NUL_ _ESC_ "[K" HEX (02) _NUL_ _NUL_ HEX (0F) _ESC_ "(a" HEX (01) _NUL_ HEX (01)</command>
   <command name="cmdTerm">_ESC_ "(a" HEX (01) _NUL_ _NUL_ _ESC_ "(b" HEX (01) _NUL_ _NUL_ _ESC_ "@"</command>
   <command name="cmdReset">_ESC_ "(a" HEX (01) _NUL_ _NUL_ _ESC_ "(b" HEX (01) _NUL_ _NUL_ _ESC_ "@"</command>
   <command name="cmdAbort">_ESC_ "(a" HEX (01) _NUL_ _NUL_ _ESC_ "(b" HEX (01) _NUL_ _NUL_ _ESC_ "@"</command>
   <command name="cmdPageEject">_FF_</command>
   <command name="cmdSetYPos">_ESC_ "(e" HEX2S(00,02) "%W"</command>
   <command name="cmdEndRasterGraphicsLine">_CR_</command>
   <command name="cmdMoveToNextRasterGraphicsLine">_ESC_ "(e" HEX (02) _NUL_ _NUL_ HEX (01)</command>
   <command name="cmdSetCompression">_ESC_ "(b" HEX2S(00,01) "%c"</command>
   <command name="cmdTransferRasterPlane">_ESC_ "(A" "%w%c"</command>
</deviceCommands>
