<deviceResolutions xmlns="http://www.ibm.com/linux/ltc/projects/omni/" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <deviceResolution>
      <name>120x72</name>
      <omniName>RESOLUTION_120_X_72</omniName>
      <xRes>120</xRes>
      <yRes>72</yRes>
      <command>_ESC_ &quot;L%w&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>8</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>60x72</name>
      <omniName>RESOLUTION_60_X_72</omniName>
      <xRes>60</xRes>
      <yRes>72</yRes>
      <command>_ESC_ &quot;K%w&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>8</resolutionScanlineMultiple>
   </deviceResolution>
</deviceResolutions>
