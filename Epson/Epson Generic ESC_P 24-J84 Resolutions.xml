<deviceResolutions xmlns="http://www.ibm.com/linux/ltc/projects/omni/" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <deviceResolution>
      <name>60x60</name>
      <omniName>RESOLUTION_60_X_60</omniName>
      <xRes>60</xRes>
      <yRes>60</yRes>
      <command>_ESC_ &quot;*&quot; _NUL_ &quot;%w&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>8</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>120x60</name>
      <omniName>RESOLUTION_120_X_60</omniName>
      <xRes>120</xRes>
      <yRes>60</yRes>
      <command>_ESC_ &quot;*&quot; HEX (02) &quot;%w&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>8</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>240x60</name>
      <omniName>RESOLUTION_240_X_60</omniName>
      <xRes>240</xRes>
      <yRes>60</yRes>
      <command>_ESC_ &quot;*&quot; HEX (03) &quot;%w&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>8</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>90x60</name>
      <omniName>RESOLUTION_90_X_60</omniName>
      <xRes>90</xRes>
      <yRes>60</yRes>
      <command>_ESC_ &quot;*&quot; HEX (06) &quot;%w&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>8</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>60x180</name>
      <omniName>RESOLUTION_60_X_180</omniName>
      <xRes>60</xRes>
      <yRes>180</yRes>
      <command>_ESC_ &quot;* %w&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>24</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>120x180</name>
      <omniName>RESOLUTION_120_X_180</omniName>
      <xRes>120</xRes>
      <yRes>180</yRes>
      <command>_ESC_ &quot;*!%w&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>24</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>90x180</name>
      <omniName>RESOLUTION_90_X_180</omniName>
      <xRes>90</xRes>
      <yRes>180</yRes>
      <command>_ESC_ &quot;*&amp;%w&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>24</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>180x180</name>
      <omniName>RESOLUTION_180_X_180</omniName>
      <xRes>180</xRes>
      <yRes>180</yRes>
      <command>_ESC_ &quot;*'%w&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>24</resolutionScanlineMultiple>
   </deviceResolution>
   <deviceResolution>
      <name>360x180</name>
      <omniName>RESOLUTION_360_X_180</omniName>
      <xRes>360</xRes>
      <yRes>180</yRes>
      <command>_ESC_ &quot;*(%w&quot;</command>
      <resolutionCapability>0</resolutionCapability>
      <resolutionDestinationBitsPerPel>1</resolutionDestinationBitsPerPel>
      <resolutionScanlineMultiple>24</resolutionScanlineMultiple>
   </deviceResolution>
</deviceResolutions>
