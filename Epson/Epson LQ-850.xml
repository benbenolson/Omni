<Device xmlns="http://www.ibm.com/linux/ltc/projects/omni/" xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" name="Epson LQ-850" xs:schemaLocation="http://www.ibm.com/linux/ltc/projects/omni/ ../OmniDevice.xsd">
   <DriverName>Epson</DriverName>
   <Capability type="MONOCHROME"/>
   <Capability type="MIRROR"/>
   <RasterCapabilities type="TOP_TO_BOTTOM"/>
   <PDL level="PDL_Epson" sublevel="LEVEL_ESC" major="1" minor="0"/>
   <Uses>Epson 24-pin 80 Col Commands.xml</Uses>
   <Has>Epson Orientations.xml</Has>
   <Has>Epson AP-3250 Resolutions.xml</Has>
   <Has>Epson 9-pin 80 Col PrintModes.xml</Has>
   <Has>Epson 24-pin 80 Col Trays.xml</Has>
   <Has>Epson LQ-850 Forms.xml</Has>
   <Has>Epson 9-pin 80 Col Medias.xml</Has>
   <Has>Epson 9-pin 80 Col Connections.xml</Has>
   <Has>Epson Copies.xml</Has>
   <Has>Epson NUps.xml</Has>
   <Has>Epson OutputBins.xml</Has>
   <Has>Epson Scalings.xml</Has>
   <Has>Epson SheetCollates.xml</Has>
   <Has>Epson Sides.xml</Has>
   <Has>Epson Stitchings.xml</Has>
   <Has>Epson Trimmings.xml</Has>
   <Instance>Epson ESC Instance.hpp</Instance>
   <Instance>Epson ESC Instance.cpp</Instance>
   <Blitter>Epson ESC Blitter.hpp</Blitter>
   <Blitter>Epson ESC Blitter.cpp</Blitter>
   <DefaultJobProperties>
      <Copies>1</Copies>
      <dither>DITHER_STUCKI_DIFFUSION</dither>
      <Form>na_letter_8.50x11.00in</Form>
      <omniForm>FORM_LETTER</omniForm>
      <media>MEDIA_PLAIN</media>
      <NumberUp>
         <NumberUp FORMAT="XbyY">
            <x>1</x>
            <y>1</y>
         </NumberUp>
         <NumberUpDirection>TorightTobottom</NumberUpDirection>
      </NumberUp>
      <Rotation>Portrait</Rotation>
      <omniOrientation>ORIENTATION_PORTRAIT</omniOrientation>
      <OutputBin>Top</OutputBin>
      <printmode>PRINT_MODE_1_ANY</printmode>
      <Resolution>180x180</Resolution>
      <omniResolution>RESOLUTION_180_X_180</omniResolution>
      <Scaling>
         <ScalingPercentage>100</ScalingPercentage>
         <ScalingType>None</ScalingType>
      </Scaling>
      <SheetCollate>SheetUncollated</SheetCollate>
      <Sides>OneSidedFront</Sides>
      <Stitching>
         <StitchingPosition>0</StitchingPosition>
         <StitchingReferenceEdge>Top</StitchingReferenceEdge>
         <StitchingType>None</StitchingType>
         <StitchingCount>0</StitchingCount>
         <StitchingAngle>0</StitchingAngle>
      </Stitching>
      <InputTray>Rear</InputTray>
      <omniTray>TRAY_REAR_CONTINUOUS</omniTray>
      <Trimming>None</Trimming>
   </DefaultJobProperties>
</Device>
